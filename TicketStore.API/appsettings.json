{
  "ConnectionStrings": {
    "TicketStoreDatabase": "Server=localhost;Port=5433;Database=ticketstore;User Id=postgres;Password=admin;SslMode=Prefer"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Oakton": "Warning",
        "Wolverine": "Warning",
        "Marten": "Warning",
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ]
  },
  "AllowedHosts": "*"
}
